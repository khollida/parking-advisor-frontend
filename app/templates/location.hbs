<div class="container">
  <div class="row">
    <div class="col-lg-8 col-lg-offset-2">
      <h3 class="page-header">Tickets Issued</h3>
      <table class="table table-hover">
        {{#each model as |ticket|}}
          <tr>
            <td>{{ticket._id}}</td>
            <td>{{ticket.location}}</td>
            <td>{{ticket.violation_date}}</td>
            <td>{{ticket.type}}</td>
            <td>{{ticket.amount}}</td>
          </tr>
        {{/each}}
      </table>
    </div>
  </div>
</div>
